{
    "require": {
        "cakephp/chronos": "^2.0",
        "php-di/php-di": "^6.3",
        "phpmailer/phpmailer": "^6.5",
        "ramsey/uuid": "^4.1",
        "selective/config": "^1.0",
        "slim/http": "^1.1",
        "slim/psr7": "^1.2",
        "slim/slim": "4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    }
}
